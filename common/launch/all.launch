<launch>
    <!-- Launch camera driver -->
    <include file="$(find camera_driver)/launch/camera.launch" />

    <!-- Launch GI-V18B IMU driver -->
    <include file="$(find gi_v18b_imu_driver)/launch/gi_v18b_imu.launch" />

    <!-- Launch Livox LiDAR with RViz -->
    <include file="$(find livox_ros_driver)/launch/livox_lidar_msg.launch" />

    <!-- Run control_collect_map.py -->
    <node pkg="common" type="control_collect_map.py" name="control_collect_map" output="log" />
    <node pkg="common" type="pub_sync_mcu.py" name="pub_sync_mcu" output="log" />
</launch>
